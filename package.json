{
  "name": "webReporting",
  "version": "1.0.0",
  "description": "Reporting app to give visibility in to the success of the CME system",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "nodemon --ignore scripts --ignore public/js/script.js ./bin/start",
    "watch": "watchify scripts/custom/{polyfills,page.min,list.min}.js scripts/{helper,header,index,routes}.js -o public/js/bundle.js -v",
    "build": "browserify scripts/custom/{polyfills,page.min,list.min}.js scripts/{helper,header,index,routes}.js | uglifyjs -mc warnings=false > public/js/bundle.min.js"
  },
  "dependencies": {
    "async": "^1.5.0",
    "autoprefixer-stylus": "^0.7.1",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.0",
    "color": "^0.11.1",
    "compression": "^1.6.1",
    "concat-files": "^0.1.0",
    "connect-assets": "^5.1.0",
    "console-stamp": "^0.2.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "http2": "^3.3.2",
    "jade": "~1.11.0",
    "mime": "^1.3.4",
    "morgan": "~1.6.1",
    "mssql": "^2.2.0",
    "multer": "^1.1.0",
    "node-minify": "^1.3.7",
    "node-watch": "^0.3.5",
    "nodemailer": "^2.1.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.3.6",
    "stylus": "0.42.3",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0",
    "xml2js": "^0.4.15"
  }
}
